#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules/vite/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules/vite/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/vite@7.3.0_@types+node@24.10.2_jiti@1.21.7_terser@5.44.1_yaml@2.8.2/node_modules:/home/runner/work/apps-in-toss-unity-sdk/apps-in-toss-unity-sdk/WebGLTemplates/AITTemplate/BuildConfig~/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
