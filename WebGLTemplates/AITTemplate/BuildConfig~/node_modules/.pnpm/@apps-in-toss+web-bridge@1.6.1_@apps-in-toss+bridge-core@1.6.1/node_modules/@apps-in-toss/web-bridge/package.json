{
  "name": "@apps-in-toss/web-bridge",
  "type": "module",
  "version": "1.6.1",
  "description": "Web Bridge for Apps In Toss",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup"
  },
  "main": "./built/index.cjs",
  "module": "./built/index.js",
  "types": "./built/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./built/index.d.ts",
        "default": "./built/index.js"
      },
      "require": {
        "types": "./built/index.d.cts",
        "default": "./built/index.cjs"
      }
    }
  },
  "files": [
    "built"
  ],
  "dependencies": {
    "@apps-in-toss/types": "1.6.1"
  },
  "devDependencies": {
    "@apps-in-toss/bridge-core": "1.6.1",
    "@apps-in-toss/framework": "1.6.1",
    "@swc/core": "^1.12.7",
    "picocolors": "^1.1.1",
    "ts-morph": "^26.0.0",
    "tsup": "^8.5.0",
    "typescript": "4.9.5",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "@apps-in-toss/bridge-core": "*"
  }
}