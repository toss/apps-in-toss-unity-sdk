{
  "name": "@apps-in-toss/web-framework",
  "type": "module",
  "version": "1.6.1",
  "description": "Web Framework for Apps In Toss",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup",
    "test": "vitest --no-watch",
    "test:no-parallel": "vitest --no-watch --config=vitest.no-parallel.config.mts"
  },
  "main": "./dist-web/index.js",
  "types": "./dist-web/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-web/index.d.ts",
      "default": "./dist-web/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./prebuilt/dev/ios": "./dist/prebuilt/dev.ios.js",
    "./prebuilt/dev/android": "./dist/prebuilt/dev.android.js",
    "./prebuilt/prod/ios": "./dist/prebuilt/prod.ios.js",
    "./prebuilt/prod/android": "./dist/prebuilt/prod.android.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "ait": "./ait.js",
    "granite": "./bin.js"
  },
  "files": [
    "dist",
    "hermesc",
    "bin.js",
    "dist-web",
    "config.d.ts"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/plugin-transform-flow-strip-types": "^7.19.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "7.18.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "@clack/prompts": "^0.10.0",
    "@hono/node-server": "^1.13.8",
    "@hono/node-ws": "^1.1.3",
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@toss/tds-react-native": "1.3.6",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7",
    "@types/debug": "^4",
    "@types/kill-port": "2.0.1",
    "@types/node": "^22.10.2",
    "@types/react": "18.3.3",
    "@types/yauzl": "^2.10.3",
    "clipanion": "^4.0.0-rc.4",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^5.1.0",
    "es-toolkit": "^1.32.0",
    "eslint": "^9.7.0",
    "execa": "9.3.0",
    "find-up-simple": "^1.0.1",
    "hono": "^4.7.4",
    "kill-port": "2.0.1",
    "picocolors": "^1.1.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "typia": "^9.3.0",
    "uuidv7": "^1.0.2",
    "vitest": "^3.0.5",
    "wait-port": "^1.1.0",
    "workspace-tools": "^0.38.2",
    "zod": "3.24.4"
  },
  "dependencies": {
    "@apps-in-toss/bridge-core": "1.6.1",
    "@apps-in-toss/cli": "1.6.1",
    "@apps-in-toss/framework": "1.6.1",
    "@apps-in-toss/plugins": "1.6.1",
    "@apps-in-toss/web-analytics": "1.6.1",
    "@apps-in-toss/web-bridge": "1.6.1",
    "@babel/core": "7.23.9",
    "@granite-js/cli": "0.1.31",
    "@granite-js/mpack": "0.1.31",
    "@granite-js/native": "0.1.31",
    "@granite-js/plugin-core": "0.1.31",
    "@granite-js/react-native": "0.1.31",
    "@granite-js/utils": "0.1.31"
  }
